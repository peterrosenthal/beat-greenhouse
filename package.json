  "dependencies": {
    "@magenta/music": "^1.23.1",
    "@types/three": "^0.136.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "esbuild": "^0.14.14",
    "eslint": "^8.7.0",
    "http-server": "^14.1.0",
